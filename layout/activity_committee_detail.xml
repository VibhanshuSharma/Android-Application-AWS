<android.support.v4.widget.DrawerLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/comms_details">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/con">


        <include layout="@layout/navigation_action"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>

        <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/textView21"
        android:layout_alignStart="@+id/chamber"
        android:id="@+id/parent_comm" />

        <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/textView23"
        android:layout_alignEnd="@+id/parent_comm"
        android:id="@+id/contact" />

        <TextView
            android:text="Office:"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="41dp"
            android:id="@+id/textView26"
            android:textColor="@color/navigationBarColor"
            android:textStyle="normal|bold"
            android:layout_below="@+id/textView23"
            android:layout_alignStart="@+id/textView19" />

        <TextView
            android:text="Name:"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="46dp"
            android:id="@+id/textView17"
            android:textColor="@color/navigationBarColor"
            android:textStyle="normal|bold"
            android:layout_below="@+id/textView"
            android:layout_alignStart="@+id/textView" />

        <TextView
            android:text="Chamber:"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="43dp"
            android:id="@+id/textView19"
            android:textColor="@color/navigationBarColor"
            android:textStyle="normal|bold"
            android:layout_below="@+id/textView17"
            android:layout_alignStart="@+id/textView17" />

        <TextView
            android:text="Committee ID:"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="36dp"
            android:id="@+id/textView"
            android:textColor="@color/navigationBarColor"
            android:textStyle="normal|bold"
            android:layout_below="@+id/star"
            android:layout_alignParentStart="true"
            android:layout_marginStart="31dp" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="50dp"
            app:srcCompat="@drawable/ic_star_b"
            android:id="@+id/star"
            android:layout_marginTop="74dp"
            android:layout_below="@+id/include"
            android:layout_alignParentEnd="true"
            android:layout_marginEnd="37dp" />

        <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
            android:id="@+id/comm_id"
            android:layout_alignBaseline="@+id/textView"
            android:layout_alignBottom="@+id/textView"
            android:layout_alignStart="@+id/star" />

        <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
            android:id="@+id/chamber"
            android:layout_alignBaseline="@+id/textView19"
            android:layout_alignBottom="@+id/textView19"
            android:layout_alignEnd="@+id/comm_name"
            android:layout_marginEnd="19dp" />

        <TextView
        android:text="TextView"
            android:id="@+id/comm_name"
            android:layout_height="65dp"
            android:layout_width="190dp"
            android:layout_below="@+id/comm_id"
            android:layout_alignEnd="@+id/star"
            android:layout_marginTop="18dp" />

        <TextView
            android:text="Contact:"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="35dp"
            android:id="@+id/textView23"
            android:textColor="@color/navigationBarColor"
            android:textStyle="normal|bold"
            android:layout_below="@+id/textView21"
            android:layout_alignStart="@+id/textView26" />

        <ImageView
            android:layout_width="wrap_content"
            app:srcCompat="@drawable/sen"
            android:id="@+id/small_pic"
            android:layout_marginEnd="12dp"
            android:layout_alignTop="@+id/chamber"
            android:layout_toStartOf="@+id/chamber"
            android:layout_height="40dp" />

        <TextView
            android:text="Parent Committee:"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView21"
            android:textColor="@color/navigationBarColor"
            android:textStyle="normal|bold"
            android:layout_below="@+id/small_pic"
            android:layout_alignStart="@+id/textView19"
            android:layout_marginTop="35dp" />

        <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
            android:id="@+id/office"
            android:layout_alignBaseline="@+id/textView26"
            android:layout_alignBottom="@+id/textView26"
            android:layout_alignStart="@+id/comm_id" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@mipmap/ic_star_black_24dp"
            android:layout_alignBottom="@+id/star"
            android:layout_alignStart="@+id/star"
            android:id="@+id/goldstar"
            android:layout_alignEnd="@+id/star"
            android:layout_alignTop="@+id/star" />

    </RelativeLayout>
</android.support.v4.widget.DrawerLayout>
